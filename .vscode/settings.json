{
  "dart.flutterSdkPath": ".fvm/versions/3.29.3",
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/.fvm": true
  },
  "files.watcherExclude": {
    "**/.dart_tool": true,
    "**/.fvm": true
  },
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "dart.lineLength": 100
}